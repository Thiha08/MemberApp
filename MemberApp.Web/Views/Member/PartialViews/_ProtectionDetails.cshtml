@model IEnumerable<MemberProtectionDetailViewModel>

<table id="datatables" class="crop-production-table table table-striped table-no-bordered table-hover dataTable dtr-inline" cellspacing="0" width="100%" style="width: 100%;" role="grid" aria-describedby="datatables_info">
    <thead>
        <tr role="row">
            <th>
                @Html.DisplayNameFor(model => model.KeyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OldValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NewValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status.ToDescription())
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Any())
        {
            foreach (var item in Model)
            {
                <tr role="row" data-protectionid="@item.Id" style="">
                    <td tabindex="0" class="sorting_1">
                        @Html.DisplayFor(modelItem => item.KeyName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.OldValue)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.NewValue)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Status.ToDescription())
                    </td>
                    <td class="td-actions text-right">
                        <a asp-controller="CropProduction" asp-action="Manage" asp-route-id="@item.Id"
                   class="btn btn-link btn-success btn-just-icon edit">
                            <i class="material-icons">clear</i>
                        </a>
                        <a asp-controller="CropProduction" asp-action="Edit" asp-route-id="@item.Id"
                   class="btn btn-link btn-warning btn-just-icon edit">
                            <i class="material-icons">done</i>
                        </a>
                    </td>
                </tr>
            }
        }
        else
        {
            <tr role="row">
                <td colspan="5" class="text-center">
                    No data found.
                </td>
            </tr>
        }
    </tbody>
</table>